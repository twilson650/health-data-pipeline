# Copy this file to backend.tf and configure with your S3 bucket details
# DO NOT commit backend.tf to version control if it contains sensitive information

terraform {
  backend "s3" {
    bucket         = "your-terraform-state-bucket"
    key            = "fhir-ml-pipeline/terraform.tfstate"
    region         = "us-west-2"
    encrypt        = true
    dynamodb_table = "terraform-state-lock"
  }
}
